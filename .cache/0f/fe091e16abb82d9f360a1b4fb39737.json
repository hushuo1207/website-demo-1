"column":9}},{"generated":{"line":66,"column":0},"source":"main.js","original":{"line":72,"column":5}},{"generated":{"line":67,"column":0},"source":"main.js","original":{"line":73,"column":1}},{"generated":{"line":67,"column":1},"source":"main.js","original":{"line":65,"column":0}},{"generated":{"line":67,"column":4},"source":"main.js","original":{"line":75,"column":0}},{"generated":{"line":68,"column":0},"source":"main.js","original":{"line":76,"column":0}},{"generated":{"line":69,"column":0},"source":"main.js","original":{"line":78,"column":0}}],"sources":{"main.js":"\r\nconst $siteList = $(\".siteList\");\r\nconst $lastLi = $siteList.find(\"li.last\");\r\nconst x = localStorage.getItem('x');\r\nconst xObject = JSON.parse(x);\r\n\r\n\r\nconst hashMap = xObject || [\r\n    { logo : 'A', localType: 'text', url: 'https://www.acfun.cn'},\r\n    {\r\n        logo : 'B',\r\n        localType: 'img',\r\n        url: 'https://www.bilibili.com'\r\n    }\r\n]\r\nconst simplifyUrl = (url) => {\r\n    return url.replace('https://', '').replace('http://', '').replace('www.','').replace('/\\/.*/', '')\r\n} //删除/ 开头之后的内容\r\n\r\nconst render = () => {\r\n    $siteList.find('li:not(.last)').remove();\r\n    hashMap.forEach((node, index) => {\r\n        const $li = $(`<li>\r\n                <div class=\"site\">\r\n                    <div class=\"logo\">${node.logo[0]}</div>\r\n                    <div class=\"link\">${simplifyUrl(node.url)}</div>\r\n                    <div class=\"close\">\r\n                        <svg class=\"icon\" >\r\n                            <use xlink:href=\"#icon-close\"></use>\r\n                        </svg>\r\n                    </div>\r\n                </div>\r\n        </li>`).insertBefore($lastLi)\r\n        $li.on('click', (e) => {\r\n            window.open(node.url)\r\n        })\r\n        $li.on('click', '.close', (e) => {\r\n            e.stopPropagation()  //阻止冒泡\r\n            hashMap.splice(index, 1)\r\n            render()\r\n        })\r\n    })\r\n}\r\nrender()\r\n\r\n$('.addButton').on('click', () => {\r\n    let url = window.prompt('请问你要输入的网址是?')\r\n    if(url.indexOf('http') !== 0) {\r\n        url = 'https://' + url\r\n    }\r\n    console.log(url)\r\n    hashMap.push({\r\n        logo: simplifyUrl(url)[0].toUpperCase(),\r\n        logoType: \"text\",\r\n        url: url\r\n    });\r\n    render();\r\n});\r\n\r\nwindow.onbeforeunload = () => {\r\n    const str = JSON.stringify(hashMap);\r\n    localStorage.setItem('x', str)\r\n}\r\n\r\n$(document).on('keypress', (e) => {\r\n    console.log(e.key)\r\n    const {key} = e;\r\n    for (let i = 0; i < hashMap.length; i++){\r\n        if (hashMap[i].logo.toLowerCase() === key){\r\n            window.open(hashMap[i].url)\r\n        }\r\n    }\r\n})\r\n\r\n//路径\r\n// parcel build src/index.html --no-minify --public-url https://.../dist\r\n\r\n//或者 parcel build src/index.html --no-minify --public-url ./\r\n"},"lineCount":null}},"error":null,"hash":"4053b271443153bc897041baec9a68fa","cacheData":{"env":{}}}